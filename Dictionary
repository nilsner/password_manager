Dictionary<string, string> My_dict1 = new Dictionary<string, string>();
            My_dict1.Add("hej", "hej");
            foreach (KeyValuePair<string, string> ele2 in My_dict1)
            {
                Console.WriteLine("{0} and {1}", ele2.Key, ele2.Value);
            }
            My_dict1.Remove();

            // After Remove() method 
            foreach (KeyValuePair<string, string> ele in My_dict1)
            {
                Console.WriteLine("{0} and {1}",
                            ele.Key, ele.Value);
            }
            Console.WriteLine();
            // Using Clear() method 
            My_dict1.Clear();

            Console.WriteLine("Total number of key/value " +
             "pairs present in My_dict:{0}", My_dict1.Count);
